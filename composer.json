{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "abr4xas/clarity-laravel": "^1.0",
        "alexpechkarev/google-maps": "^11.0",
        "anaseqal/nova-import": "^0.0.6",
