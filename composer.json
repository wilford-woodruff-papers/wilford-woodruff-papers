        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "filament/spatie-laravel-tags-plugin": "^3.2",
        "flowframe/laravel-trend": "^0.2",
        "genealabs/laravel-caffeine": "^11.0",
        "guzzlehttp/guzzle": "^7.8",
        "http-interop/http-factory-guzzle": "^1.2",
        "intervention/image": "^2.7",
        "jeffersonsimaogoncalves/nova-permission": "dev-nova-4-compatibility",
        "jessedp/php-timezones": "^0.2.3",
        "laravel/framework": "^11.23",
        "laravel/horizon": "^5.22",
        "laravel/jetstream": "^5.0",
        "laravel/nova": "^4.0",
        "laravel/octane": "^2.3",
        "laravel/prompts": "^0.1.6",
        "laravel/pulse": "^1.0@beta",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.8",
        "laravel/socialite": "^5.11",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^3.3",
        "maantje/pulse-database": "^0.1.0",
        "maatwebsite/excel": "^3.1",
        "maatwebsite/laravel-nova-excel": "^1.3",
        "mailersend/laravel-driver": "^2.4",
        "maize-tech/laravel-markable": "^2.2",
        "meilisearch/meilisearch-php": "^1.4.1",
        "mews/purifier": "^3.4",
        "misd/linkify": "^1.1",
        "mtvs/eloquent-hashids": "^3.5",
        "openai-php/laravel": "^0.9",
        "owen-it/laravel-auditing": "^13.6",
        "phpoffice/phpword": "^1.2",
        "pixelcreation/nova-field-sortable": "^3.2",
        "psr/simple-cache": "^3.0",
        "pusher/pusher-php-server": "^7.2",
        "ramsey/uuid": "^4.2",
        "rpj/daterangepicker": "^0.0.7",
        "saloonphp/laravel-http-sender": "^3.0",
        "saloonphp/laravel-plugin": "^3.5",
        "saloonphp/rate-limit-plugin": "^2.0",
        "saloonphp/saloon": "^3.0",
        "saloonphp/xml-wrangler": "^1.2",
        "sammyjo20/lasso": "^3.3",
        "sentry/sentry-laravel": "^4.3",
        "socialiteproviders/facebook": "^4.1",
        "socialiteproviders/google": "^4.1",
        "spatie/browsershot": "^3.60",
        "spatie/eloquent-sortable": "^4.2",
        "spatie/icalendar-generator": "^2.5",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-backup": "^8.6",
        "spatie/laravel-deleted-models": "^1.0",
        "spatie/laravel-honeypot": "^4.5",
        "spatie/laravel-login-link": "^1.2",
        "spatie/laravel-medialibrary": "^11.3",
        "spatie/laravel-permission": "^6.2",
        "spatie/laravel-ray": "^1.35",
        "spatie/laravel-schemaless-attributes": "^2.5",
        "spatie/laravel-sluggable": "^3.6",
        "spatie/laravel-tags": "^4.6",
        "spatie/nova-tags-field": "^4.0",
        "spatie/regex": "^3.1",
        "spiral/roadrunner-cli": "^2.6.0",
        "spiral/roadrunner-http": "^3.3.0",
        "staudenmeir/belongs-to-through": "^2.16",
        "tapp/filament-auditing": "^3.0",
        "tightenco/parental": "^1.4",
        "unisharp/laravel-filemanager": "^2.8",
        "vinkla/hashids": "^12.0",
        "voku/portable-utf8": "^6.0",
        "voku/simple_html_dom": "^4.8",
        "vormkracht10/laravel-open-graph-image": "^v0.7",
        "wildside/userstamps": "^2.4",
        "wilford-woodruff-papers/laravel-socialite-familysearch": "^1.0",
        "wire-elements/modal": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.10",
        "fakerphp/faker": "^1.23",
        "laravel/dusk": "^8.0",
        "laravel/envoy": "^2.9",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "laravel/telescope": "^5.0",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "openai-php/client": "^0.6.1",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "pestphp/pest-plugin-watch": "^2.0",
        "spatie/laravel-horizon-watcher": "^1.1",
        "phpunit/phpunit": "^11.0.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "cweagans/composer-patches": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        },
        "patches": {
            "paquettg/php-html-parser": {
                "Fix deprecation notices for offsetGet() return type": "patches/paquettg_phphtmlparser_fix_offsetget_deprecation.patch"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "php artisan nova:publish",
            "@php artisan horizon:publish --ansi",
            "@php artisan telescope:publish --ansi",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/Items"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bernhardh/laravel-nova-date-range-filter.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/jonfackrell/nova-permission.git"
        }
    ]
}
