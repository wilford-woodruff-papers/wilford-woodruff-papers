{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "alexpechkarev/google-maps": "^8.0",
        "anaseqal/nova-import": "^0.0.6",
        "calebporzio/parental": "v1.0.0",
        "chris-ware/nova-breadcrumbs": "^2.3",
        "dmitrybubyakin/nova-medialibrary-field": "^3.3",
        "dyrynda/laravel-efficient-uuid": "^4.3",
        "dyrynda/laravel-model-uuid": "^6.4",
        "emilianotisato/nova-tinymce": "^1.2",
        "fideloper/proxy": "^4.4",
        "froala/nova-froala-field": "^3.4",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.12",
        "laravel/horizon": "^5.9",
        "laravel/jetstream": "^2.6",
        "laravel/nova": "3.30.0",
        "laravel/sanctum": "^2.6",
        "laravel/telescope": "^4.7",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.0",
        "maatwebsite/excel": "^3.1",
        "maatwebsite/laravel-nova-excel": "^1.2",
        "mailersend/laravel-driver": "^1.0",
        "owen-it/laravel-auditing": "^12.0",
        "paquettg/php-html-parser": "^3.1",
        "pos-lifestyle/laravel-nova-date-range-filter": "^1.3",
        "ramsey/uuid": "^4.2",
        "spatie/eloquent-sortable": "^3.11",
        "spatie/laravel-activitylog": "^4.4",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-permission": "^4.0",
        "spatie/laravel-ray": "^1.29",
        "spatie/laravel-sluggable": "^2.6",
        "spatie/laravel-tags": "^4.3",
        "spatie/nova-tags-field": "^3.4",
        "syrian-open-source/laravel-youtube-iframe-generator": "^1.1",
        "unisharp/laravel-filemanager": "^2.2",
        "wwp/items": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^6.13",
        "laravel/envoy": "^2.7",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "php artisan nova:publish",
            "@php artisan horizon:publish --ansi",
            "@php artisan telescope:publish --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/Items"
        }
    ]
}
